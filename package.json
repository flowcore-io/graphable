{
  "name": "graphable",
  "version": "1.2.3",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "bunx @biomejs/biome lint .",
    "lint:fix": "bunx @biomejs/biome lint --write .",
    "format": "bunx @biomejs/biome format --write .",
    "format:check": "bunx @biomejs/biome format .",
    "services:up": "docker-compose up -d",
    "services:down": "docker-compose down",
    "db:push": "drizzle-kit push",
    "flowcore:apply:production": "flowcore apply --v2 -f flowcore.yml && flowcore scenario apply -f flowcore.scenario.yml",
    "flowcore:apply:dev": "flowcore apply --v2 -f flowcore.yml -f flowcore.local.yml",
    "flowcore:stream": "flowcore scenario local -f flowcore.scenario.yml -f flowcore.scenario.local.yml -f flowcore.local.development.yml -s now -e http://localhost:3001/api -H \"X-Secret: 1234\"",
    "flowcore:backfill": "flowcore scenario local -f flowcore.scenario.yml -f flowcore.scenario.local.yml -f flowcore.local.development.yml -s first -t 30000 -e http://localhost:3000/api -H \"X-Secret: 1234\"",
    "flowcore:backfill-open": "flowcore scenario local -f flowcore.scenario.yml -f flowcore.scenario.local.yml -f flowcore.local.development.yml -t 30000 -e http://localhost:3000/api -H \"X-Secret: 1234\""
  },
  "dependencies": {
    "@azure/identity": "^4.13.0",
    "@azure/keyvault-secrets": "^4.10.0",
    "@base-ui/react": "^1.0.0",
    "@flowcore/pathways": "^0.16.4",
    "@flowcore/sdk": "^1.63.0",
    "@flowcore/sdk-transformer-core": "^2.5.1",
    "@hookform/resolvers": "^3.9.1",
    "@monaco-editor/react": "^4.7.0",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@types/pg": "^8.16.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "expr-eval": "^2.0.2",
    "gridstack": "^12.4.1",
    "lucide-react": "^0.561.0",
    "next": "16.0.10",
    "next-auth": "^4.24.11",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "react": "19.2.1",
    "react-day-picker": "^9.12.0",
    "react-dom": "19.2.1",
    "react-hook-form": "^7.68.0",
    "recharts": "2.15.4",
    "shadcn": "^3.6.0",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "ulid": "^3.0.2",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.8",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
