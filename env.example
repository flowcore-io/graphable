# NextAuth Configuration
NEXTAUTH_SECRET=your-secret-key-minimum-32-characters-long
NEXTAUTH_URL=http://localhost:3000

# Usable OIDC Configuration
USABLE_OIDC_ISSUER=https://auth.flowcore.io/realms/memory-mesh
USABLE_CLIENT_ID=your-client-id
USABLE_CLIENT_SECRET=your-client-secret

# Database Configuration
# For local development with docker-compose (port 5436):
DATABASE_URL=postgresql://postgres:postgres@localhost:5436/graphable
# For production or other setups, update accordingly

# Node Environment (optional, defaults to development)
NODE_ENV=development

# Usable API Configuration
USABLE_API_BASE_URL=https://usable.dev/api

# Flowcore Configuration
FLOWCORE_TENANT=your-tenant-name
FLOWCORE_DATACORE=your-datacore-name
FLOWCORE_WEBHOOK_BASE_URL=http://localhost:3000
FLOWCORE_WEBHOOK_API_KEY=your-api-key
FLOWCORE_TRANSFORMER_SECRET=your-secret-key
