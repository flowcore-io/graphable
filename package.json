{
  "name": "graphable",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "services:up": "docker-compose up -d",
    "services:down": "docker-compose down",
    "db:push": "drizzle-kit push",
