    "@t3-oss/env-nextjs": "^0.13.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "lucide-react": "^0.561.0",
    "next": "16.0.10",
    "next-auth": "^4.24.11",
    "postgres": "^3.4.7",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "shadcn": "^3.6.0",
