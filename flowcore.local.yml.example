apiVersion: data-core.flowcore.io/v1
kind: DataCore
metadata:
  name: graphable
  tenant: <your-tenant>
spec:
  deleteProtection: false

---

apiVersion: data-core.flowcore.io/v1
kind: FlowType
metadata:
  name: graphable.tenant.0
  tenant: <your-tenant>
spec:
  dataCore: graphable
  description: "Graphable tenant management events"
  eventTypes:
    - name: tenant.linked.0
      description: "A workspace was linked to a user as a Graphable tenant"
    - name: tenant.unlinked.0
      description: "A workspace was unlinked from a user"

---
apiVersion: data-core.flowcore.io/v1
kind: FlowType
metadata:
  name: graphable.graph.0
  tenant: <your-tenant>
spec:
  dataCore: graphable
  description: "Graphable graph lifecycle events"
  eventTypes:
    - name: graph.created.0
      description: "Graph fragment was created"
    - name: graph.updated.0
      description: "Graph fragment was updated"
    - name: graph.deleted.0
      description: "Graph fragment was deleted"

---
apiVersion: data-core.flowcore.io/v1
kind: FlowType
metadata:
  name: graphable.dashboard.0
