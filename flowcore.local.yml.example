spec:
  dataCore: graphable
  description: "Graphable dashboard lifecycle events"
  eventTypes:
    - name: dashboard.created.0
      description: "Dashboard fragment was created"
    - name: dashboard.updated.0
      description: "Dashboard fragment was updated"
    - name: dashboard.deleted.0
      description: "Dashboard fragment was deleted"

---
apiVersion: data-core.flowcore.io/v1
kind: FlowType
metadata:
  name: graphable.folder.0
  tenant: <your-tenant>
spec:
  dataCore: graphable
  description: "Graphable folder lifecycle events"
  eventTypes:
    - name: folder.created.0
